   <string notr="true">/* ==== GENERAL ==== */
QWidget {
    background-color: #2d2d2d;
    color: #e0e0e0;
    font-family: &quot;Segoe UI&quot;, &quot;Helvetica Neue&quot;, sans-serif;
    font-size: 12px;
}

/* ==== QPushButton ==== */
QPushButton {
    background-color: #3c3f41;
    border: 1px solid #5a5a5a;
    border-radius: 4px;
    padding: 6px 12px;
}
QPushButton:hover {
    background-color: #505357;
}
QPushButton:pressed {
    background-color: #2a2a2a;
}

/* ==== QLabel ==== */
QLabel {
    color: #dcdcdc;
}

/* ==== QLineEdit ==== */
QLineEdit {
    background-color: #3a3a3a;
    border: 1px solid #5a5a5a;
    border-radius: 4px;
    padding: 4px;
    color: #ffffff;
}

/* ==== QComboBox ==== */
QComboBox {
    background-color: #3a3a3a;
    border: 1px solid #5a5a5a;
    border-radius: 4px;
    padding: 4px;
    color: #ffffff;
}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-left: 1px solid #5a5a5a;
}
QComboBox QAbstractItemView {
    background-color: #2d2d2d;
    color: #f0f0f0;
    selection-background-color: #505357;
}

/* ==== QRadioButton ==== */
QRadioButton {
    spacing: 6px;
}
QRadioButton::indicator {
    width: 14px;
    height: 14px;
}
QRadioButton::indicator:unchecked {
    border: 1px solid #aaa;
    border-radius: 7px;
    background-color: #3a3a3a;
}
QRadioButton::indicator:checked {
    border: 1px solid #aaa;
    border-radius: 7px;
    background-color: #66aaff;
}

/* ==== QListWidget ==== */
QListWidget {
    background-color: #2f2f2f;
    border: 1px solid #5a5a5a;
    border-radius: 4px;
    padding: 4px;
    color: #ffffff;
}
QListWidget::item:selected {
    background-color: #505357;
    color: white;
}

/* ==== QScrollBar (for QListWidgets, QTextEdit, etc.) ==== */
QScrollBar:vertical {
    background: #2d2d2d;
    width: 10px;
    margin: 0px;
}
QScrollBar::handle:vertical {
    background: #505357;
    border-radius: 4px;
    min-height: 20px;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background: none;
    height: 0px;
}
